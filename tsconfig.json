    "moduleResolution": "node",
    "noEmit": true,
    "skipLibCheck": true,
    "resolveJsonModule": true
  },
  "exclude": [
    "__tests__/**/*-test.ts"
],
  "extends": "@tsconfig/react-native/tsconfig.json"
}